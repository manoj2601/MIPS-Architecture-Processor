fibonacci:
3	sub $sp $sp $t1				00000011101010011110100000100010		
4	SW $ra, 2($sp)				10101111101111110000000000000010			
5	sw $s0, 1($sp)				10101111101100000000000000000001
6	sw $s1, 0($sp)				10101111101100010000000000000000
7	add $s0, $a0 $zero			00000000100000001000000000100000			

